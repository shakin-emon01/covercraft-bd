{
  "name": "server",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon --exec \"node --require ts-node/register src/index.ts\" --ext ts",
    "build": "node -e \"require('fs').rmSync('dist',{ recursive:true, force:true })\" && tsc",
    "start": "npx prisma generate && npx puppeteer browsers install chrome && npx prisma db push --accept-data-loss && node dist/src/index.js",
    "seed": "ts-node prisma/seed.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "archiver": "^7.0.1",
    "axios": "^1.13.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^2.1.0",
    "nodemailer": "^8.0.1",
    "puppeteer": "^24.37.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/express-rate-limit": "^6.0.2",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^7.0.11",
    "@types/puppeteer": "^7.0.4",
    "nodemon": "^3.0.0",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
